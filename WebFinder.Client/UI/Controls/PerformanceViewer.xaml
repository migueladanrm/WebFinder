<UserControl x:Class="WebFinder.UI.Controls.PerformanceViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WebFinder.UI.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="250"
             MinWidth="200" UseLayoutRounding="True">
    <Border Background="White" BorderBrush="#FFC8C8C8" BorderThickness="1,0,0,0">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Label Content="Rendimiento" FontFamily="Segoe UI Semibold" FontSize="15" Foreground="Gray" HorizontalAlignment="Center" Margin="0,5,0,0" VerticalAlignment="Top"/>
            <StackPanel x:Name="stkItems" Grid.Row="1">

            </StackPanel>
        </Grid>
    </Border>
</UserControl>
